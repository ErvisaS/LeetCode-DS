class Solution {
    public boolean containsDuplicate(int[] nums) {
     Arrays.sort(nums);
      for(int i=0;i<nums.length-1;i++){
          if(nums[i]==nums[i+1])
              
                   return true;
                 }
                   
    return false;
      }  
    }


//Runtime: 3 ms, faster than 99.56% of Java online submissions for Contains Duplicate.
//Memory Usage: 42.3 MB, less than 90.29% of Java online submissions for Contains Duplicate.
